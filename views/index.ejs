<%- include('partials/head', {title:" Maxico Srl - Posis 2.0" }) %>

    <body>
        <div class="container">
            <!-- menu de navegacion -->
            <%- include('partials/navbar') %>
          
            <!-- animacion de uploading -->
            <%- include('partials/uploading') %>

            <!-- mensajes de informacion -->
            <%- include('partials/message') %>

        </div>
        <script type="text/javascript">
 
            function handleCloseMessage (closeIcon){
      
                document.querySelector('.message').classList.add('hidden');
             }

            function handleSendData() {
                document.querySelector('.uploading').style.display = "block";
             }
   
        </script>
        
        <% if (locals.alert ) { %>
        <script>                   
         
         Swal.fire({
                title: "Secuencia enviada!",
                icon: "success",
                });
        </script>
        <% } %>

        <% if (locals.register ) { %>
            <script>                   
              Swal.fire({
                position: 'center',
                icon: 'success',
                title: 'Registro correcto!',
                showConfirmButton: false,
                timer: 1500
        })
            </script>
            <% } %>

    </body>
</html>
